{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Production",
      "program": "${workspaceFolder}/src/app.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "production"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Staging",
      "program": "${workspaceFolder}/src/app.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "staging"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Development",
      "program": "${workspaceFolder}/src/app.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Local",
      "program": "${workspaceFolder}/src/app.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "local"
      },
    }
  ]
}