    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.2.0",
    "@coreui/icons": "^0.1.1",
    "@coreui/react": "^2.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.5",
    "core-js": "^2.5.6",
    "debug": "^4.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.4.0",
    "react-loading": "^2.0.3",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "react-toastify": "^4.1.0",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "simple-line-icons": "^2.4.1"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "set REACT_APP_STAGE='LOCAL' && npm-run-all -p watch-css start-js",
    "build": "set REACT_APP_STAGE='LOCAL' && npm-run-all build-css build-js",
    "start:dev": "set REACT_APP_STAGE='DEV' && npm-run-all -p watch-css start-js",
    "build:dev": "set REACT_APP_STAGE='DEV' && npm-run-all build-css build-js",
    "start:homol": "set REACT_APP_STAGE='HML' && npm-run-all -p watch-css start-js",
    "build:homol": "set REACT_APP_STAGE='HML' && npm-run-all build-css build-js",
    "start:prod": "set REACT_APP_STAGE='PROD' && npm-run-all -p watch-css start-js",
    "build:prod": "set REACT_APP_STAGE='PROD' && npm-run-all build-css build-js",
    "lint": "eslint src/** --fix"
  }
}
